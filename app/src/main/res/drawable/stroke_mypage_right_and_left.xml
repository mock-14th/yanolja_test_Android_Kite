<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="-1dp"
        android:left="0dp"
        android:right="0dp"
        android:top="-1dp">
        <shape android:shape="rectangle" >

            <!-- 테두리 두께 및 색상 설정 -->
            <stroke
                android:width="0.5dp"
                android:color="@color/strokeColor" />

            <solid android:color="#FFFFFF" />
        </shape>
    </item>

</layer-list>

